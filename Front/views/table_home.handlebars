

  <div class="grid-container">
    <aside class="left"></aside>
    <main class="center">
      <div id="message-add">
        <span>Um novo Usuario foi adicionado!</span>
      </div>
      <div class="search">
        <h3 style="text-align: center;">Tabela de Dados</h3>
        <div>
          <select name="" id="searchselect" style="margin-right: 20px; border-radius:10px; padding:5px">
            <option value="gender">Gender</option>
             <option value="age">Age</option>
             <option value="city">City</option>
             <option value="state">State</option>
             <option value="cpf">CPF</option>
          </select>
          <input type="text" name="search" class="pesquisar" placeholder="search...">
          <input type="submit" id="btnsearch" value="Pesquisar">
          <input type="hidden" id="tipo" value="">
        </div>
      </div>
      <table class="table">
        <thead>
          <tr>
            <th class="name">Nome</th>
            <th>Gender</th>
            <th>Age</th>
            <th>City</th>
            <th>State</th>
            <th>E-mail</th>
            <th>CPF</th>
            <th class="phone">Phone</th>
          </tr>
        </thead>
        <tbody id="">
          {{#each dados}}
          {{#each results}}
          <tr>
            <td>{{name.first}} {{name.last}}</td>
            <td>{{gender}}</td>
            <td>{{dob.age}}</td>
            <td>{{location.city}}</td>
            <td>{{location.state}}</td>
            <td>{{email}}</td>
            <td>{{id.value}}</td>
            <td>{{phone}}</td>
          </tr>
           {{/each}}
          {{/each}}
        </tbody>
      </table>
    </main>
    <aside class="right"></aside>
  </div>
  </div>  
  <script src="https://code.jquery.com/jquery-3.7.1.min.js"></script>
  <script src="script.js"></script>

</body>