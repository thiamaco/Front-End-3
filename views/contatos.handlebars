      </div>
      <div id="contatos">
        {{#each lista}}
            <div class="dados">
                <div>
                    <img src={{foto}} alt="">
                </div>
                <div class="cont">
                    <div>
                        <h3>{{nome}}</h3>
                        <h6>{{phone}}</h6>
                        <h6>{{city}}</h6>
                    </div>
                </div>
            </div>
             {{/each}}
             
      </div>
    </main>
    <aside class="right"></aside>
  </div>
  </div>
